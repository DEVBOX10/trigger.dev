{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@trigger.dev/core/v3": ["../../packages/core/src/v3"],
      "@trigger.dev/core/v3/*": ["../../packages/core/src/v3/*"],
      "@trigger.dev/coordinator": ["../../internal-packages/coordinator/src"],
      "@trigger.dev/coordinator/*": ["../../internal-packages/coordinator/src/*"]
    }
  }
}
